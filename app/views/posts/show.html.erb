<h1> <%= @post.title %> </h1>
<h3> <%= @post.body %> </h3>
<% @post.photos.each do | photo | %>
		<%= image_tag(photo.url, size: "800x600")%>	<br> <br>
	<% end %>
<% if user_signed_in? %>
  <% if @post.user_id == current_user.id %>
    <hr>
    <%= link_to "Edit", edit_post_path %> |
    <%= link_to "Delete", post_path(@post.id), method: :delete, data: {confirm: "Are you sure?"} %>
  <% end %>
<% end %>
